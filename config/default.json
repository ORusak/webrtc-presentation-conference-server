{

    /**
     *Параметры логирования
     */
    "logger": {
        
        /**
         * Уровень логирования
         */
        "level": "info",

        /**
         * Глубина разворачивания объектов в логах
         */
         "depth": 2
    },

    /**
     * Настройки запуска сервера
     */
    "server": {

        /**
         *Порт запуска сервера
         */
        "port": 8080,

        /**
         * Тип окружения:
         * - production
         * - development
         * - test
         */
         "env": "production"
    }
}
